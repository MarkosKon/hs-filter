{
  "name": "hs-filter",
  "private": true,
  "description": "The most advanced Hearthstone tool for filtering your collection. Supports completely custom filters and includes a deck builder.",
  "author": "Markos Konstantopoulos <markoskonstanto@hotmail.com>",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx --fix src",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "netlify deploy --dir=public",
    "deploy:prod": "netlify deploy --dir=public --prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ]
  },
